<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1">
    <BorderPane>
        <top>
            <HBox alignment="TOP_CENTER" BorderPane.alignment="TOP_CENTER">
                <Text fx:id="leaderboardText" strokeType="OUTSIDE" strokeWidth="0.0" text="LEADERBOARD"/>
            </HBox>
        </top>
        <center>
            <VBox fx:id="CenterBox" alignment="CENTER" minWidth="400.0" prefHeight="200.0" prefWidth="100.0">
                <ScrollPane fx:id="scrollPaneFilters" minHeight="300.0" prefWidth="260.0">
                    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="70.0">
                        <VBox fx:id="playersBox" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0"
                              spacing="30.0">
                            <Text fx:id="playersText" strokeType="OUTSIDE" strokeWidth="0.0" text="Players"/>
                        </VBox>
                        <VBox fx:id="pointsBox" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0"
                              spacing="30.0">
                            <Text fx:id="pointsText" strokeType="OUTSIDE" strokeWidth="0.0" text="Points"/>
                        </VBox>
                    </HBox>
                </ScrollPane>
                <VBox fx:id="BottomBox" alignment="BOTTOM_CENTER" minHeight="250.0" spacing="50.0">
                    <Text fx:id="winningText" strokeType="OUTSIDE" strokeWidth="0.0" text="Winning Text"/>
                    <Button fx:id="doneButton" mnemonicParsing="false" text="Done"/>
                </VBox>
            </VBox>
        </center>
    </BorderPane>
</AnchorPane>
